<%content_tag_for(:tr,district) do -%>
<td>
    <%=link_to(highlight(district.name,params[:search]),admin_district_path(district)) %>
</td>
<td>
    <%=h district.state.name %>
</td>
<td>
    <%=h district.bidding_zone.name %>
</td>

<td>
    <%=h district.description %>
</td>

<td>
    <%=h @created_name %>
</td>
<td>
    <%=h district.created_at.strftime('%d-%m-%Y %H:%M:%S') %>
</td>
<td>

    <%= link_to 'Edit', edit_admin_district_path(district) %> |

    <%= link_to 'Destroy', [:admin,district], :confirm => 'Are you sure?', :method => :delete %> |
    <%= link_to 'Show', [:admin,district] %>
</td>
<% end %>

