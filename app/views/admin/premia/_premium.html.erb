<%content_tag_for(:tr,premium) do -%>
<td>
    <%=link_to(highlight(premium.name,params[:search]),admin_premium_path(premium)) %>
</td>

<td>
    <%=h @created_name %>
</td>
<td>
    <%=h premium.created_at.strftime('%d-%m-%Y') %>
</td>
<td>

    <%= link_to 'Edit', edit_admin_premium_path(premium) %>|

    <%= link_to 'Delete', [:admin,premium], :confirm => 'Are you sure?', :method => :delete %>|
    <%= link_to 'View', [:admin,premium] %>
</td>
<% end %>

