<%content_tag_for(:tr,state) do -%>
<td>
    <%=link_to(highlight(state.name,params[:search]),admin_state_path(state)) %>
</td>
<td>
    <%=h state.description %>
</td>
<td>
    <%=h @created_name %>
</td>
<td>
    <%=h state.created_at.strftime('%d-%m-%Y %H:%M:%S') %>
</td>
<td>

    <%= link_to 'Edit', edit_admin_state_path(state) %> |

    <%= link_to 'Destroy', [:admin,state], :confirm => 'Are you sure?', :method => :delete %> |
    <%= link_to 'Show', [:admin,state] %>
</td>
<% end %>

