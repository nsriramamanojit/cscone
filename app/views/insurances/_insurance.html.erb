<%content_tag_for(:tr,insurance) do -%>
<td><%=link_to(highlight(insurance.name,params[:search]),insurance_path(insurance)) %></td>
 <td><%=h insurance.policy_number%></td>
<td><%=h link_to(insurance.premium.name,insurance_path(insurance)) %></td>
<td><%=h insurance.amount %></td>
<td><%=h insurance.remarks %></td>
 <td>
  <%= link_to 'Edit', edit_insurance_path(insurance) %> |
 <%= link_to 'Destroy', insurance, :confirm => 'Are you sure?', :method => :delete %> |
    <%= link_to 'Show', insurance %>
</td>
<% end %>

