<div class="form-container">
  <fieldset>
        <legend>
            <b><%= @vle_user.name %></b>
        </legend>
        <div>
            <label>
                <b>Name:</b>
            </label>
            <%=h @vle_user.name%>
        </div>
        <hr/>
        <div>
            <label>
                <b>Email:</b>
            </label>
            <%=h @vle_user.email%>
        </div>
        <hr/>
         
         <div>
            <label>
                <b>Phone Number:</b>
            </label>
            <%=h @vle_user.phone_number %>
        </div>
        <hr/>
         <div>
            <label>
                <b>Father Name:</b>
            </label>
            <%=h @vle_user.vle_representative_profile.father_name %>
        </div>
        <hr/>
        <div>
            <label>
                <b>Address:</b>
            </label>
            <%=h @vle_user.vle_representative_profile.address %>
        </div>
        <hr/>
        <div>
            <label>
                <b>Pincode:</b>
            </label>
            <%=h @vle_user.vle_representative_profile.pincode %>
        </div>
        <hr/>
        <div>
            <label>
                <b>Date Of Birth:</b>
            </label>
            <%=h @vle_user.vle_representative_profile.date_of_birth %>
        </div>
        <hr/>
        <div>
            <label>
                <b>CSC Panchyath:</b>
            </label>
            <%=h @vle_user.vle_representative_profile.csc_panchayat %>
        </div>
        <hr/>
</div>
<div class="bottom_links">
     &nbsp;&nbsp;
    <%= link_to :controller=>'/bankings',:action=>'edit_profile',:id=>current_user.id do %>
    Edit
    <% end %>
 </div>

